- if @owners_employee.errors.any?
  #error_explanation
    %h2= "#{pluralize(@owners_employee.errors.count, "error")} prohibited this owners_employee from being saved:"
    %ul
      - @owners_employee.errors.full_messages.each do |msg|
        %li= msg

.field
  = f.label :given_name
  = f.text_field :given_name
.field
  = f.label :last_name
  = f.text_field :last_name
.field
  = f.label :email
  = f.text_field :email
.field
  = f.label :password
  = f.password_field :password
.field
  = f.label :password_confirmation
  = f.password_field :password_confirmation
.actions
  = f.submit 'Save'
